# Tomb Planet 02
entity = {
	name = "nuked_planet_02_entity"
	pdxmesh = "nuked_planet_01_mesh"
	
	meshsettings = {
	name = "planet_geosphereShape"
	texture_diffuse = "ig_arid_07_nuked_diffuse.dds"
	texture_normal = "ig_arid_07_nuked_normal.dds"
	texture_specular = "ig_arid_07_nuked_specular.dds"
	}

	##cloud texture override
	meshsettings = {
	name = "clouds_geosphereShape"
	texture_diffuse = "ig_clouds_tomb_01_diffuse.dds"
	texture_normal = "ig_clouds_tomb_01_normal.dds"
	texture_specular = "nospec.dds"
	}
	
	##cloud shadow texture override
	meshsettings = {
	name = "clouds_shadow_geosphereShape"
	texture_diffuse = "ig_clouds_tomb_01_shadow_diffuse.dds"
	texture_normal = "nonormal.dds"
	texture_specular = "nospec.dds"
	}

	default_state = "idle"
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_planet_class" } }
	}
}
